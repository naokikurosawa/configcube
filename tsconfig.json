{
  "compilerOptions": {
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "removeComments": true,
    "rootDir": "./front_app/javascripts",
    "sourceMap": true,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types",
      "./front_app/types"
    ]
  }
}
